{
    "manifest_version": 3,
    "name": "Send URL to Local Scraper",
    "version": "1.0",
    "description": "Sends the current tab URL to a local scraping backend",
    "permissions": ["activeTab"],
    "host_permissions": ["http://127.0.0.1/*"],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_title": "Send URL"
    }
}